- if @collaborator.destroyed?
  $("#userid-#{@collaborator.user_id}").html('#{link_to "Add Collaborator", collaborators_path(collaborator: {user_id: @collaborator.user_id, wiki_id: @collaborator.wiki_id}), method: :post, id: "userid-#{@collaborator.user_id}", remote: true}');
- else
  $('.flash').prepend("
  .alert.alert-danger>
    %button.close{"data-dismiss" => "alert", :type => "button"} &times;
    = flash.now[:alert]
  ");
